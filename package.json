  "author": "Conor Breen",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "ora": "^7.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0"
  }
}
