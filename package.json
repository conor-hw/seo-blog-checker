{
  "name": "seo-blog-checker",
  "version": "1.0.0",
  "description": "Node.js script to evaluate SEO quality of WordPress blog posts using Gemini AI",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "evaluate": "node src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "validate-config": "node src/utils/config-validator.js"
